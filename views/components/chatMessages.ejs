<section id="messages">
  <% if (data.messages != undefined) { %> <% for (let i = 0; i <
  data.messages.length; i++) { %> <% if(data.messages[i].media == 'text') { %>
  <% if (data.messages[i].sender === data.clientid) { %>

  <div class="message messageSend">
    <p>
      <%= data.messages[i].content %>
    </p>
  </div>

  <% } else { %>

  <div class="message messageRecieve">
    <p>
      <%= data.messages[i].content %>
    </p>
  </div>

  <% } %> <% } else if(data.messages[i].media == 'gif'){ %> <% if
  (data.messages[i].sender === data.clientid) { %>

  <div class="message messageSend">
    <img src="<%= data.messages[i].content %>" id="gifImg" />
  </div>

  <% } else { %>

  <div class="message messageRecieve">
    <img src="<%= data.messages[i].content %>" />
  </div>

  <% } %> <% } %> <% } %> <% } else { %>
  <div class="message messageRecieve">
    <p>No messages found, start chatting and matching ğŸ¦€</p>
  </div>

  <div class="message messageSend">
    <p>No messages found, start chatting and matching ğŸ¦€</p>
  </div>
  <% } %>
</section>
